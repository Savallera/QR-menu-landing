{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "for web development",
  "main": "gulpfile.js",
  "type": "module",
  "author": "Savallera",
  "license": "MIT",
  "private": false,
  "scripts": {
    "check": "prettier --check './dist/**/*.{html,js,css}'",
    "format": "prettier --write './dist/**/*.{html,js,css}'",
    "lint": "stylelint './dist/**/*.css'",
    "lintfix": "stylelint --fix './dist/**/*.css'",
    "checkAll": "npm run check && npm run lint",
    "fixAll": "npm run format && npm run lintfix",
    "deploy": "gh-pages -d dist",
    "start": "gulp"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "autoprefixer": "^10.4.17",
    "babel": "^6.23.0",
    "browser-sync": "^2.29.3",
    "cssnano": "^6.0.5",
    "del": "^7.1.0",
    "gh-pages": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-avif": "^1.1.1",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssimport": "^7.0.0",
    "gulp-file-include": "^2.3.0",
    "gulp-fonter": "^0.3.0",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-imagemin": "^9.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-svg-sprite": "^2.0.3",
    "gulp-ttf2woff2": "^4.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^5.0.0",
    "gulp-webp-html": "^1.0.2",
    "html-minifier": "^4.0.0",
    "postcss": "^8.4.35",
    "postcss-combine-media-query": "^1.0.1",
    "prettier": "^3.2.5",
    "sass": "^1.69.5",
    "stylelint": "^16.2.1",
    "webpack": "^5.89.0",
    "webpack-stream": "^7.0.0"
  },
  "browserslist": [
    "last 10 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
